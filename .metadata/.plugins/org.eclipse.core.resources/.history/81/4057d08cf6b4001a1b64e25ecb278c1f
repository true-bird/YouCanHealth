
$(document).ready(function(){
	$(".daySports").click(function() {
		$("#sportsSeq").val($(this).find('.seq').text());
		$("#sportsInfo").submit();
	});
	
	$("#routineEnroll").submit(function(e) {
		if(sessionStorage.getItem("userId") == "") {
			alert("로그인이 필요합니다.");
			e.preventDefault;
		}
	});
	
//	href="<%=contextPath%>/sports/sportsDetails?seq=${shoulder.seq}"
});