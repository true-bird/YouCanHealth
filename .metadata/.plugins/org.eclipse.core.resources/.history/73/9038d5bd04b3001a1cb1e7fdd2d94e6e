package com.matnagu.myHell.comment.dto;

public class CommentDto {
	private int seq;//시퀀스
	private String id;//작성자 아이디
	private String content;//댓글 내용
	private int communityseq;//댓글 달리는 글 시퀀스
	private String commentdate;//댓글 작성일
	
	public CommentDto() {
		
	}

	public int getSeq() {
		return seq;
	}

	public void setSeq(int seq) {
		this.seq = seq;
	}

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getContent() {
		return content;
	}

	public void setContent(String content) {
		this.content = content;
	}

	public int getCommunityseq() {
		return communityseq;
	}

	public void setCommunityseq(int communityseq) {
		this.communityseq = communityseq;
	}

	public String getCommentdate() {
		return commentdate;
	}

	public void setCommentdate(String commentdate) {
		this.commentdate = commentdate;
	}
	
}
