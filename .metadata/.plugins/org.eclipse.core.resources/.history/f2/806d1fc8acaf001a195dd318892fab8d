

$(document).ready(function(){
	// process..
//	alert("!!");
	$("select[name='category']").change(function() {
		alert($(this).val());
		$.ajax({
			url: "routines",
			type: "post",
			data: { category: $(this).val()},
			cache: false,
			success: function(data){
//				alert(12);
			}
		});
	});
	
});