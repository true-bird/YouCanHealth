

$(document).ready(function(){
	// process..
//	alert("!!");
	$("select[name='category']").change(function() {
		alert($(this).val());
		$.ajax({
			url: "routinesAj",
			type: "POST",
			data: { category: $(this).val()},
			cache: false,
			success: function(data){
				alert(12);
				for(var i =0 : data.routineList)
			}
		}).done(function(fragment) {
			$("#list").replaceWith(fragment);
		});
	});
	
});