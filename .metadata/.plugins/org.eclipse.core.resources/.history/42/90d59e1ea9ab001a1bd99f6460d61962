<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="guestbookMapper">


	<select id="selectGuestbookAllList" resultType="com.matnagu.myHell.geustbook.dto.GuestbookDto">
	SELECT  SEQ, 
	                NAME,
	                ADDR,
	                GENDER,	<!-- DECODE(GENDER,'M','남','W','여') AS GENDER, -->		                
	                TO_CHAR(REGDATE, 'YYYY-MM-DD HH24:MI:SS') AS REGDATE
	FROM    GUESTBOOK
	ORDER BY NAME ASC
	</select>

</mapper>
