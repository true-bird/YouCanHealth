

$(document).ready(function(){
	$(".section-background.background-image").css("background-image","url('/myHell/images/routine/chest.jpg')");
	$("select[name='category']").change(function() {
		$("#list").load("routineList",{ category: $(this).val()});
		$(".section-background.background-image").css("background-image","url('/myHell/images/routine/chest.jpg')");
	});
	
});