

$(document).ready(function(){
	$('ul.tabs li').click(function(){
		var tab_id = $(this).attr('data-tab');
		$('ul.tabs li').removeClass('current');
		$('.tab-content').removeClass('current');

		$(this).addClass('current');
		$("#"+tab_id).addClass('current');
	});
	
	$('.add').click(function(){
		alert(document.getElementById("pInput").innerHTML);
		child = window.open("selectSports","child","width=400,height=300");
		alert(child.document.getElementById("cInput").innerHTML);

		child.document.getElementById("cInput").innerHTML = "ddd";
	});
	function setChildText(){
        
    }


});